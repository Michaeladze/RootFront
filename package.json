{
  "name": "root-front",
  "version": "1.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "email": "michael.kutateladze@gmail.com",
    "name": "Michael Kutateladze",
    "url": "https://github.com/Michaeladze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michaeladze/RootFront.git"
  },
  "keywords": [
    "UI Components"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc && yarn build_css",
    "build_css": "node-sass src/ -o dist/ ",
    "start": "npm run build -- -w",
    "commit": "git add . && npx git-cz",
    "storybook": "start-storybook -p 9010 -s public -c src/.storybook",
    "build-storybook": "build-storybook -s public -c ",
    "rflink": "yarn link  && cd node_modules/react && yarn link && cd ../react-dom && yarn link && cd ../react-router-dom && yarn link",
    "rfunlink": "yarn unlink  && cd node_modules/react && yarn unlink && cd ../react-dom && yarn unlink && cd ../react-router-dom &&yarn unlink",
    "stylelint": "npx stylelint **/*.scss --fix",
    "g": "node node_modules/react-generator-cmd/dist",
    "release": "release-it --no-git.requireCleanWorkingDir"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.21.0",
    "axios-observable": "^1.1.3",
    "browser-image-compression": "^1.0.13",
    "detect-browser": "^5.2.0",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "react-date-picker": "^8.0.1",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-observable": "^1.2.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "use-reactive-form": "^1.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt": "8.3.5",
    "@ptsecurity/commitlint-config": "^1.0.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-input-mask": "^2.0.5",
    "@types/redux-actions": "^2.6.1",
    "@types/storybook__addon-info": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.16.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-unused-imports": "^1.0.1",
    "git-cz": "4.7.6",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "10.2.9",
    "react": "17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "17.0.1",
    "react-generator-cmd": "^1.4.1",
    "redux-devtools-extension": "^2.13.8",
    "release-it": "^14.2.2",
    "rimraf": "^3.0.2",
    "storybook-addon-designs": "^5.4.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "swiper": "6.1.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint ./src --format=table --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "npx stylelint ./src/**/*.scss --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  },
  "release-it": {
    "github": {
      "release": false
    },
    "npm": {
      "publishPath": "dist"
    }
  }
}
