{
  "presets": ["@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining",
    [
      "babel-plugin-react-scoped-css",
      {
        "include": ".(sa|sc|c)ss$",
        "hashSeed": "hashSeed"
      }
    ]
  ]
}
